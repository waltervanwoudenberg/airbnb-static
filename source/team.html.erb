<div class="container text-center padded">
  <h1>Our team</h1>
    <ul class="list-inline text-center">
      <% data.flats.each do |owner, flat| %>
        <li>
          <%= image_tag flat.owner_picture, class: "img-circle", width: "200px" %>
          <h3><%= owner.capitalize %></h3>
        </li>
      <% end %>
    </ul>
    <h3 class="padded">
    <a href="mailto:contact@airbnb.com">
      <i class="fa fa-envelope"></i> contact@airbnb.com
    </a>
  </h3>
</div>
